	if(PlayerInfo[playerid][TerroristRank] < 20)
	{
	    PlayerInfo[playerid][TerroristRank] ++;
	    SendClientMessage(playerid,COLOR_DODGERBLUE,"Your terrorist level has increased. Check out /terskill to see your level and what you can blow up next.");
	}
	return 1;
}

