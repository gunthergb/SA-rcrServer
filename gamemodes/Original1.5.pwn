stock Float:restfloat(Float:dividend, Float:divisor)
{
    new r[2];
    new s[10];
    r[0] = floatround(1000 * dividend, floatround_ceil);
    r[1] = floatround(1000 * divisor, floatround_ceil);
    valstr(s, (r[0] % r[1]), false);
    s[strlen(s)] = '0';
    new len = strlen(s);
    s[len-2] = s[len-3];
    s[len-3] = s[len-4];
    s[len-4] = '.';
    return floatstr(s);
}

stock InteriorVW(playerid)
{
    new Float:PosX, Float:PosY, Float:PosZ; GetPlayerPos(playerid, PosX, PosY, PosZ);
    new Float:PosSum = (PosX + PosY + PosZ) - restfloat((PosX + PosY + PosZ), 1);                       
    new PosSumInt = 0;    
    if (PosSum < 0)    
    {
        PosSumInt = floatround((0-PosSum), floatround_round);   
    }    
    else    
    {
        PosSumInt = floatround((PosSum), floatround_round);    
    }
    new VW = ((PosSumInt/10)%65999)*10;
    return VW;
}

stock SystemMsg(playerid, msg[])
{
	if((IsPlayerConnected(playerid))&&(strlen(msg)>0))
	{
		SendClientMessage(playerid, COLOR_SYSTEM, msg);
	}
	return 1;
}

stock CreateStrip(playername[], Float:x,Float:y,Float:z,Float:Angle)
{
	for(new i = 0; i < sizeof(SpikeInfo); i++)
	{
		if(SpikeInfo[i][sCreated] == 0)
		{
			SpikeInfo[i][sCreated] = 1;
			SpikeInfo[i][sX] = x;
			SpikeInfo[i][sY] = y;
			SpikeInfo[i][sZ] = z - 0.7;
			SpikeInfo[i][sObject] = CreateObject(2899, x, y, z - 0.9, 0, 0, Angle - 90);
			//SpikeInfo[i][Name] = strval(playername);
			format(SpikeInfo[i][Name], MAX_PLAYER_NAME-1, "%s", playername);
			return 1;
		}
	}
	return 1;
}

stock DeleteAllStrip()
{
	for(new i = 0; i < sizeof(SpikeInfo); i++)
	{
		if(SpikeInfo[i][sCreated] == 1)
		{
			SpikeInfo[i][sCreated] = 0;
			SpikeInfo[i][sX] = 0.0;
			SpikeInfo[i][sY] = 0.0;
			SpikeInfo[i][sZ] = 0.0;
			DestroyObject(SpikeInfo[i][sObject]);
		}
	}
	return 1;
}

stock DeleteClosestStrip(playerid)
{
	for(new i = 0; i < sizeof(SpikeInfo); i++)
	{
		if(IsPlayerInRangeOfPoint(playerid, 2.0, SpikeInfo[i][sX], SpikeInfo[i][sY], SpikeInfo[i][sZ]))
		{
			if(SpikeInfo[i][sCreated] == 1)
			{
				new string[64];
				SpikeInfo[i][sCreated]=0;
				SpikeInfo[i][sX]=0.0;
				SpikeInfo[i][sY]=0.0;
				SpikeInfo[i][sZ]=-5000.0; // we change 0.0 to -5000.0 to be sure it wont work in the middle of the SA map, where x=y=z ~ 0.0
				DestroyObject(SpikeInfo[i][sObject]);
				format(string, sizeof(string)-1, "You have removed the spike placed by: %s", SpikeInfo[i][Name]);
				SendClientMessage(playerid, 0xFF0000FF, string);
				return 1;
			}
		}
	}
	return 1;
}

encode_tires(tires1, tires2, tires3, tires4)
{
	return tires1 | (tires2 << 1) | (tires3 << 2) | (tires4 << 3);
}

stock GetPlayerSpeed(playerid, bool:kmh) // by misco
{
    new Float:Vx, Float:Vy, Float:Vz, Float:rtn;
    if(IsPlayerInAnyVehicle(playerid))
	{
		GetVehicleVelocity(GetPlayerVehicleID(playerid), Vx, Vy, Vz);
	}
	else
	{
		GetPlayerVelocity(playerid, Vx, Vy, Vz);
	}
	rtn = floatsqroot(floatabs(floatpower(Vx + Vy + Vz, 2)));
    return kmh?floatround(rtn * 100 * 1.61):floatround(rtn * 100);
}

stock strrest(const string[], &index)
{
	new length = strlen(string);
	while ((index < length) && (string[index] <= ' '))
	{
		index++;
	}
	new offset = index;
	new result[128];
	while ((index < length) && ((index - offset) < (sizeof(result) - 1)))
	{
		result[index - offset] = string[index];
		index++;
	}
	result[index - offset] = EOS;
	return result;
}

