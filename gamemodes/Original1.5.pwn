	CreateExplosion(playerx,playery,playerz,2,1000.00);
	CreateExplosion(playerx,playery,playerz,2,1000.00);
	CreateExplosion(playerx,playery,playerz,2,1000.00);
	SetPlayerHealth(playerid, 0);
	HasBlownVehicleRecently[playerid] = 1;
	oscore = GetPlayerScore(playerid);
   	SetPlayerScore(playerid, oscore +1);
	format(string,sizeof(string),"Terrorist %s(%d) has killed himself in a suicide bombing.",PlayerName(playerid));
