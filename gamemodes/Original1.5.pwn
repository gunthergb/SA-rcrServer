// Radiation Poisoning
forward GivePlayerRadPoisoning();
public GivePlayerRadPoisoning()
{
    for(new i=0; i<MAX_PLAYERS_; i++)
	{
 		SendClientMessage(i, COLOR_RED, "You have caught radiation poisoning. You need to get to a hospital immediately.");
		SetPlayerDrunkLevel(i, 30000);
		RadPoisoning[i] = 1;
	}
}
forward HasRadPoisoning();
public HasRadPoisoning()
{
	for(new i = 0; i < MAX_PLAYERS; i++)
	{
		new Float:health;
		GetPlayerHealth(i, health);
		if(RadPoisoning[i] == 1 && health > 5 && IsSpawned[i] == 1 && IsPlayerConnected(i))
		{
			SetPlayerHealth(i, health-5);
		}
		else if(RadPoisoning[i] == 1 && health <= 5 && IsSpawned[i] == 1 && IsPlayerConnected(i))
		{
			new string[256];
			new victim[24];
			SetPlayerHealth(i, health-5);
			GetPlayerName(i,victim, 24);
			format(string, sizeof(string), "%s(%d) Has died from Radiation Poisoning.",victim,i);
			SendClientMessageToAll(0xB22222AA,string);
		}
	}
}
