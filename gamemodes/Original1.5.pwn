CMD:robnude(playerid, params[])
{
	new string[128];
    if(IsSpawned[playerid] == 0)
	{
		SendClientMessage(playerid, COLOR_ERROR, "You are dead. You cannot use this command");
    	return 1;
    }
    if(Jailed[playerid] == 1)
	{
		SendClientMessage(playerid, COLOR_ERROR, "You are dead. You cannot use this command");
    	return 1;
    }
    if(cuffed[playerid] == 1)
	{
		SendClientMessage(playerid, COLOR_ERROR, "You cannot use this command while you are handcuffed");
    	return 1;
    }
    if(gTeam[playerid] == TEAM_COP || gTeam[playerid] == TEAM_FIREMAN || gTeam[playerid] == TEAM_ARMY || gTeam[playerid] == TEAM_SWAT || gTeam[playerid] == TEAM_FBI || gTeam[playerid] == TEAM_MEDIC || gTeam[playerid] == TEAM_CASSEC || gTeam[playerid] == TEAM_JAILTK)
	{
		SendClientMessage(playerid,COLOR_ERROR,"You cannot rob the Nude Strippers Daily");
		return 1;
 	}
 	if(NudeCPRobbedRecent >= 1)
	{
		SendClientMessage(playerid, COLOR_ERROR, "Nude Strippers Daily has been robbed recently. Try again later.");
	 	return 1;
	}
 	new sexrand = random(1000);
 	if(sexrand >=0 && sexrand <=10)
 	{
	 	SendClientMessage(playerid, 0xA9A9A9AA, "|_Nude Strippers Daily Robbery Failed_|");
	 	SendClientMessage(playerid,COLOR_ERROR,"Your attempt to rob the Nude Strippers Daily has failed");
	 	return 1;
 	}
    if(sexrand >=11 && sexrand <=1000)
	{
	    if(AbleToRobNudeCP[playerid] == 0)
   		{
   		    SendClientMessage(playerid,COLOR_ERROR,"You are not at the robbing Clucking Bell Restaurant checkpoint.");
   		    return 1;
		}
   		else
   		{
	        new cluckrobbber[30];
	        new pcol = GetPlayerColor(playerid);
	        GetPlayerName(playerid,cluckrobbber,30);
	        new plwl = GetPlayerWantedLevel(playerid);
		    SetPlayerWantedLevel(playerid, plwl +4);
			RobbingNudeCP[playerid] =30;
			SendClientMessage(playerid,0x00C7FFAA,"You are now robbing the Nude Strippers Daily. The Police have been dispatched and will be on route right now");
			plwl = GetPlayerWantedLevel(playerid);
			cluckbellrobbedrecent = 240;
			SendClientMessage(playerid, 0xA9A9A9AA, "|_Crime Commited_|");
			format(string, sizeof(string), "(NUDE ROBBERY) Wanted Level %d",plwl);
			SendClientMessage(playerid,pcol,string);
			commitedcrimerecently[playerid] +=120;
			printf("%s(%d) has started a Nude Strippers Daily robbery",cluckrobbber,playerid);
			for(new i=0;i<MAX_PLAYERS;i++)
			{
	        	if(LawEnforcementRadio[i] == 1)
				{
	        		new string1[256];
	        		new string2[256];
					format(string1, sizeof(string1), "DISPATCH: (ROBBERY IN PROGRESS) Suspect: %s(%d)",cluckrobbber,playerid);
					format(string2, sizeof(string2), "ALL UNITS: Please respond to Nude Strippers Daily and arrest %s(%d)",cluckrobbber,playerid);
					SendClientMessage(i, COLOR_ROYALBLUE, string1);
					SendClientMessage(i, COLOR_ROYALBLUE, string2);
			    }
		    }
	    }
	}
	return 1;
}

