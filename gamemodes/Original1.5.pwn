CMD:fakedeath(playerid, params[])
{
    if(PlayerInfo[playerid][AdminLevel] == 1337)
	{
	    #define Killer 0
	    #define Killed 1

	    new Fake[2], Weapon;
	    if(sscanf(params, "uuD(-1)", Fake[Killer], Fake[Killed], Weapon))
			return SendClientMessage(playerid,COLOR_ERROR, "/fakemsg""[Killer] [Killer] [WeaponID]");
		else
		{
		    if(Fake[Killer] == 0xFFFF) 							return SendClientMessage(playerid,COLOR_ERROR, "Killer is not connected!");
		    if(Fake[Killed] == 0xFFFF) 							return SendClientMessage(playerid,COLOR_ERROR, "Killed is not connected!");
		    if(Fake[Killer] == 0xFFFF && Fake[Killed] == 0xFFFF) return SendClientMessage(playerid,COLOR_ERROR, "Killer and Killed are both offline!");

		    if(Weapon == -1)
		    {
		        new rWeapon;
		        switch (random(2))
		        {
		            case 0: rWeapon = random(19);
		            case 1: rWeapon = 21 + random(26);
		            default: rWeapon = 0;
		        }
		        Weapon = rWeapon;
		        return SendDeathMessage(Fake[Killer], Fake[Killed], Weapon),
					   SendClientMessage(playerid, COLOR_ERROR, "Fake death sent!");
			}

			if(!IsValidWeapon(Weapon))
				return SendClientMessage(playerid,COLOR_ERROR, "Invalid Weapon ID!");

	        SendDeathMessage(Fake[Killer], Fake[Killed], Weapon);

			SendClientMessage(playerid, COLOR_ERROR, "Fake death sent!");
		}
	}
	return 1;
}

IsValidWeapon(weaponid)
{
	switch(weaponid)
	{
	    case 1..18: return 1;
	    case 22..46: return 1;
	}
    return 0;
}

