forward RobbingArmyCountdown();
public RobbingArmyCountdown()
{
    for(new i=0; i < MAX_PLAYERS; i++)
    {
        if(IsPlayerConnected(i))
        {
            if(robbingarmy[i] >= 2)
            {
				robbingarmy[i] --;
				new robtimer;
				robtimer = (robbingarmy[i]);
                new string[100];
                format(string, sizeof(string), "~y~ROBBERY IN PROGRESS~n~~b~STAY IN THE CHECKPOINT~n~~w~ROBBERY COMPLETE IN~r~ %d~w~ SECONDS",robtimer);
                GameTextForPlayer(i, string, 2000,3);
			}
			if(robbingarmy[i] == 1)
            {
                new armyrobrand = random(150000);
				robbingarmy[i] =0;
				new arobbber[30];
                new string[100];
                new pcol = GetPlayerColor(i);
                GetPlayerName(i,arobbber,30);
                format(string, sizeof(string), "%s(%d) Has robbed $%d from the Army Base",arobbber,i,armyrobrand);
                SendClientMessageToAll(0x00C7FFAA,string);
                format(string, sizeof(string), "~b~Army Base~n~~y~ROBBERY COMPLETE~n~~w~$%d",armyrobrand);
                GameTextForPlayer(i, string, 5000,3);
                TogglePlayerControllable(i, 1);
                GivePlayerMoney(i,armyrobrand);
                format(string, sizeof(string), "Army Base Robbery Complete. You robbed a total of $%d",armyrobrand);
                SendClientMessage(i,pcol,string);
                if(robberrank[i] <=39)
				{
	    			SendClientMessage(i,COLOR_WHITE,"Your robbing skill level has been increased. Type /robskill for more info");
	    			robberrank[i] +=1;
