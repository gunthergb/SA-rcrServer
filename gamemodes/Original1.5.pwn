	if(playerid != giveplayerid)
	{
	    if(pmsoff[playerid] == 1)
		{
		    SendClientMessage(playerid, COLOR_ERROR, "This person has refused to accept any Personal Messages");
			return 1;
		}
		else
		{
			format(msg,sizeof(msg),"%s",msg);
			SendClientMessage(playerid,COLOR_DEADCONNECT, "|_Message Sent_|");
			format(sentmsg,sizeof(sentmsg),">>%s(%d): %s", PlayerName(giveplayerid), giveplayerid, msg);
			SendClientMessage(playerid,COLOR_GREEN,sentmsg);
			SendClientMessage(giveplayerid,COLOR_DEADCONNECT, "|_Message Received_|");
			format(receivemsg,sizeof(receivemsg),"**%s(%d): %s", PlayerName(playerid), playerid, msg);
			SendClientMessage(giveplayerid,COLOR_GREEN,receivemsg);
			PlayerPlaySound(giveplayerid,1085,0.0,0.0,0.0);
			printf(receivemsg);
			return 1;
		}
	}
	else
	{
	    SendClientMessage(playerid, COLOR_ERROR, "You cannot PM yourself");
	    return 1;
	}
}

