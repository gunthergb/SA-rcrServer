			return 1;
		}
		if(giveplayerid == playerid)
		{
	    	SendClientMessage(playerid,COLOR_ERROR,"You cannot bail yourself. Why would you waste your time?");
	    	return 1;
		}
    	if(IsSpawned[playerid] == 0)
	 	{
			 SendClientMessage(playerid, COLOR_ERROR, "You are dead. You cannot use this command");
		     return 1;
     	}
	 	if(Jailed[playerid] == 1)
	 	{
			 SendClientMessage(playerid, COLOR_ERROR, "You cannot use this command in jail");
		     return 1;
     	}
     	if(giveplayerid == playerid)
		{
		    SendClientMessage(playerid,COLOR_ERROR,"You cannot bail yourself.");
		    return 1;
		}
     	if(gTeam[playerid] == TEAM_ARMY || gTeam[playerid] == TEAM_SWAT || gTeam[playerid] == TEAM_FBI)
	 	{
		    SendClientMessage(playerid,COLOR_ERROR,"Law Enforcement agents cannot bail people,use /parole");
