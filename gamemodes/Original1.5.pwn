CMD:sellairport(playerid, params[])
{
    new giveplayerid;
	new string[128];
	if(sscanf(params, "i", giveplayerid))
	{
		SendClientMessage(playerid,COLOR_ERROR,"USAGE: /sellairport [Playerid]");
		return 1;
	}
	if(!IsPlayerConnected(giveplayerid))
	{
		format(string,sizeof(string),"The Player ID (%d) is not connected to the server. You cannot sell the airport to him.",giveplayerid);
	    SendClientMessage(playerid,COLOR_ERROR,string);
		return 1;
	}
	if(giveplayerid == playerid)
	{
	    SendClientMessage(playerid,COLOR_ERROR,"You cannot sell it to yourself. Why would you waste your time?");
	    return 1;
	}
    if(IsSpawned[playerid] == 0)
	{
		SendClientMessage(playerid, COLOR_ERROR, "You are dead. You cannot use this command");
	    return 1;
    }
    if(Jailed[playerid] == 1)
	{
		SendClientMessage(playerid, COLOR_ERROR, "You cannot use this command in jail");
	    return 1;
    }
    AirPortOwner[playerid]=dUserINT(PlayerName(playerid)).("AirPortOwner");
	if(AirPortOwner[playerid] == 0)
	{
		SendClientMessage(playerid,COLOR_ERROR,"You are not the current owner of the Air Port");
	    return 1;
    }
    if(AirPortOwner[playerid] == 1337)
	{
    	if(!udb_Exists(PlayerName(giveplayerid)))
		{
		    SendClientMessage(playerid,COLOR_ERROR,"That player does not have any account on our server. Cannot sell to that player");
		    return 1;
    	}
    	if(!PLAYERLIST_authed[giveplayerid])
		{
		    SendClientMessage(playerid,COLOR_ERROR,"That player is not logged into their account. Cannot sell to that player");
		    return 1;
    	}
	    new oldowner[30];
	    new newowner[30];
	    GetPlayerName(playerid,oldowner,30);
	    GetPlayerName(giveplayerid,newowner,30);
	    if(GetDistanceBetweenPlayers(playerid,giveplayerid) > 10)
		{
		    format(string, sizeof(string), "%s(%d) Is not close enough. You cannot give that player the official papers",newowner,giveplayerid);
		    SendClientMessage(playerid, COLOR_ERROR, string);
		    return 1;
   		}
	    AirPortOwner[playerid] =0;
	    dUserSetINT(PlayerName(playerid)).("AirPortOwner",AirPortOwner[playerid]);
	    AirPortOwner[giveplayerid] =1337;
	    dUserSetINT(PlayerName(giveplayerid)).("AirPortOwner",AirPortOwner[giveplayerid]);
	    SendClientMessage(playerid, 0xA9A9A9AA, "|_AirPort Business Sold_|");
	    format(string, sizeof(string), "You have sold the AirPort business to %s(%d) - You are no longer the owner of The Air Port",newowner,giveplayerid);
	    SendClientMessage(playerid,0x00C7FFAA, string);
	    SendClientMessage(giveplayerid, 0xA9A9A9AA, "|_Air Port Business Purchased_|");
	    format(string, sizeof(string), "You have purchased the Air Port business from %s(%d) | Use /sellairport (id) to sell the business",oldowner,playerid);
	    SendClientMessage(giveplayerid,0x00C7FFAA, string);
	    SendClientMessage(giveplayerid,0x00C7FFAA,"If a player buys a ticket from the Airport, you will get the full amount of the sale in your pocket");
	    SendClientMessage(giveplayerid,0x00C7FFAA,"You will only be paid if you are online and logged into your account at the time of the sale");
	    format(string, sizeof(string), "AirPort owner %s(%d) has sold the AirPort business to %s(%d)",oldowner,playerid,newowner,giveplayerid);
	    SendClientMessageToAll(0x00C7FFAA, string);
	    printf("**(AirPort Sold)** %s(%d) has sold the AirPort to %s(%d)",oldowner,playerid,newowner,giveplayerid);
    }
	return 1;
}

