public OnPlayerDisconnect(playerid, reason)
{
    new leaveMsg[128], name[MAX_PLAYER_NAME], reasonMsg[8];
	switch(reason)
	{
		case 0: reasonMsg = "Timeout";
		case 1: reasonMsg = "Leaving";
		case 2: reasonMsg = "Kicked";
	}
	GetPlayerName(playerid, name, sizeof name);
    if (_:discordChannel == 0)
		discordChannel = DCC_FindChannelById(DISCORD_CHANNEL_ID); // Discord channel ID

	format(leaveMsg, sizeof leaveMsg, "Player %s has left the server. (%s)", name, reasonMsg);
	DCC_SendChannelMessage(discordChannel, leaveMsg);
    zoneupdates[playerid] = 0;
	player_zone[playerid] = -1;
	Banning[playerid] = 0;
	Kicking[playerid] = 0;
	HasDrugHouseKeys[playerid] =0;
    if(PLAYERLIST_authed[playerid])
	{
	    dUserSetINT(PlayerName(playerid)).("money",GetPlayerMoney(playerid));
	    dUserSetINT(PlayerName(playerid)).("score",GetPlayerScore(playerid));
	    dUserSetINT(PlayerName(playerid)).("robrank",PlayerInfo[playerid][RobRank]);
	    dUserSetINT(PlayerName(playerid)).("teamkills",teamkiller[playerid]);
	    dUserSetINT(PlayerName(playerid)).("innocentkills",innocentkiller[playerid]);
	    dUserSetINT(PlayerName(playerid)).("lowwantedkills",lowwantedkiller[playerid]);
	    dUserSetINT(PlayerName(playerid)).("cashinsured",BankRobInsurance[playerid]);
	    dUserSetINT(PlayerName(playerid)).("RegularPlayer",PlayerInfo[playerid][RegularPlayer]);
	    dUserSetINT(PlayerName(playerid)).("HasDrugHouseKeys",HasDrugHouseKeys[playerid]);
		// Terrorist
	    dUserSetINT(PlayerName(playerid)).("terroristrank",PlayerInfo[playerid][TerroristRank]);
	    dUserSetINT(PlayerName(playerid)).("TazeRank",PlayerInfo[playerid][Tazes]);
	    dUserSetINT(PlayerName(playerid)).("CuffRank",PlayerInfo[playerid][Cuffs]);
	    dUserSetINT(PlayerName(playerid)).("ArrestRank",PlayerInfo[playerid][Arrests]);
	    dUserSetINT(PlayerName(playerid)).("KidnapperRank",PlayerInfo[playerid][KidnapperRank]);
	    // End of Terrorist
		//Briefcase
		dUserSetINT(PlayerName(playerid)).("BriefcaseMoney",PlayerInfo[playerid][BriefcaseMoney]);
	    dUserSetINT(PlayerName(playerid)).("BriefcaseC4",PlayerInfo[playerid][BriefcaseC4]);
	    dUserSetINT(PlayerName(playerid)).("BriefcaseWeaponSlot1",PlayerInfo[playerid][BriefcaseWeaponSlot1]);
	    dUserSetINT(PlayerName(playerid)).("BriefcaseWeaponAmmoSlot1",PlayerInfo[playerid][BriefcaseWeaponAmmoSlot1]);
	    dUserSetINT(PlayerName(playerid)).("BriefcaseWeaponSlot2",PlayerInfo[playerid][BriefcaseWeaponSlot2]);
	    dUserSetINT(PlayerName(playerid)).("BriefcaseWeaponAmmoSlot2",PlayerInfo[playerid][BriefcaseWeaponAmmoSlot2]);
	    dUserSetINT(PlayerName(playerid)).("BriefcaseWeaponSlot3",PlayerInfo[playerid][BriefcaseWeaponSlot3]);
	    dUserSetINT(PlayerName(playerid)).("BriefcaseWeaponAmmoSlot3",PlayerInfo[playerid][BriefcaseWeaponAmmoSlot3]);
	    dUserSetINT(PlayerName(playerid)).("Briefcase",PlayerInfo[playerid][HasBriefcase]);
	}
  	PLAYERLIST_authed[playerid] = false;
	// Terrorist
	PlantingYugoBridge[playerid] = 0;
	PlantingBank[playerid] = 0;
	PlantingArmyGate[playerid] = 0;
	PlantingControlDoor[playerid] = 0;
	Explosions[playerid] = 0;
	HasNukeKeys[playerid] = 0;
    new ConnectName[30], str[100];
	GetPlayerName(playerid, ConnectName, 30);
    switch (reason)
	{
        case 0: format(str, 256, "%s has left the server! (Timeout)", ConnectName);
		case 1: format(str, 256, "%s has left the server! (Leaving)", ConnectName);
		case 2: format(str, 256, "%s has left the server! (Kicked)", ConnectName);
    }
    SendClientMessageToAll(0x808080AA, str);
    return 1;
}

