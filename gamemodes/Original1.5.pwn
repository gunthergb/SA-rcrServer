{
	if(classid == 0 || classid == 1 || classid == 2 || classid == 3 || classid == 4 || classid == 5  || classid == 10 || classid == 11 || classid == 12)
	{
		gTeam[playerid] = TEAM_COP;
	}
	else if(classid == 6)
	{
